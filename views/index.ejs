<%- include header %>
<% posts.forEach(function(post, index){ %>
<p><h2><a href="#"><%= post.title %></a></h2></p>
<p class="info">
  author: <a href="#"><%= post.name %></a> | 
  date: <%= moment(post.createdAt).format('YYYY-MM-DD HH-mm-ss') %>
</p>
<p><%- post.content %></p>
<% }) %>
<%- include footer %>
